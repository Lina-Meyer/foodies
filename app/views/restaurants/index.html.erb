<div class="container">
  <div class="row">
    <div class="col-sm-8 col-sm-offset-2">
      <div id="restaurants">
        <% @restaurants.each do |restaurant| %>
          <h4><%= restaurant.name %></h4>
          <p><%= restaurant.city %></p>
          <% restaurant.users.each do |r| %>
            <p><%= r.email %></p>
          <% end %>
        <% end %>
      </div>
    </div>
  </div>
</div>
